<div id="gems">
  <h4>Gems upgraded</h4>

  <table class="table table-condensed table-responsive" style="width:auto">
    <tbody>
    <%- GemUpgrades.parse(details).each do |gem_name, upgrade| -%>
      <tr>
        <td><%= gem_name %></td>
        <td><%= upgrade[:from] %></td>
        <th>&rarr;</th>
        <td><%= upgrade[:to] %></td>
      </tr>
    <%- end -%>
    </tbody>
  </table>
</div>
