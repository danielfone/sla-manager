<div id="exceptions">
  <h4>Exceptions addressed</h4>
  <ul class="list-group">
    <% @report.exceptions.each do |entry| %>
      <li class="list-group-item list-group-item-success">
        <span class="glyphicon glyphicon-ok text-success"></span>
        <%= entry.note %>
      </li>
    <% end %>
  </ul>
</div>
